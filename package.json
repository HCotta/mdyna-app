{
  "name": "dinagram",
  "version": "1.0.0",
  "description": "Task notification and productivity app",
  "main": "index.js",
  "scripts": {
    "build-webpack": "rm -rf ./dist && webpack --optimize-minimize --config webpack.config.js",
    "start": "rm -rf ./dist && NODE_ENV=DEV concurrently \"webpack-dev-server --hot --inline\" \"sleep 2 && electron ./main.js\"",
    "watch": "webpack-dev-server --hot --inline",
    "build-electron": "NODE_ENV=PROD webpack --config webpack.config.electron.js;"
  },
